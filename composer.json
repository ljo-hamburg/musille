{
  "name": "ljo-hamburg/musille",
  "description": "The Musille WordPress Theme.",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ljo-hamburg/wp-concerts"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "timber/timber": "^1.19.1",
    "yahnis-elsts/plugin-update-checker": "^4.11"
  },
  "require-dev": {
    "johnpbloch/wordpress": "^5.8.3",
    "squizlabs/php_codesniffer": "^3.6.2",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "wp-coding-standards/wpcs": "^2.3.0",
    "wp-cli/wp-cli": "^2.5.0",
    "wp-cli/i18n-command": "^2.2.11",
    "ljo-hamburg/wp-concerts": "dev-master"
  },
  "authors": [
    {
      "name": "Kim Wittenburg",
      "email": "codello@wittenburg.kim",
      "homepage": "https://github.com/codello"
    }
  ],
  "autoload": {
    "psr-4": {
      "LJO\\Musille\\": "includes/"
    }
  },
  "config": {
    "allow-plugins": {
      "johnpbloch/wordpress-core-installer": true,
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
